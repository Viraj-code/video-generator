{
    "version": 2,
    "buildCommand": "vite build",
    "outputDirectory": "dist/public",
    "functions": {
      "server/index.ts": {
        "runtime": "@vercel/node@3"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.ts"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/server/index.ts"
      }
    ]
  }